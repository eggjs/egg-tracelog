{
  "name": "egg-tracer",
  "version": "1.1.0",
  "description": "tracer for egg",
  "eggPlugin": {
    "name": "tracer"
  },
  "main": "lib/tracer.js",
  "files": [
    "lib",
    "app",
    "config",
    "agent.js",
    "index.js",
    "app.js"
  ],
  "keywords": [
    "egg",
    "egg-plugin",
    "eggPlugin",
    "tracer",
    "traceId",
    "spanId",
    "parentSpanId"
  ],
  "dependencies": {
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "autod": "^3.1.0",
    "egg": "^2.24.0",
    "egg-bin": "^4.14.0",
    "egg-ci": "^1.13.0",
    "egg-mock": "^3.24.1",
    "eslint": "^6.5.1",
    "eslint-config-egg": "^7.5.1",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": ">=6.0.0"
