{
  "name": "egg-tracer",
  "version": "1.0.0",
  "description": "tracer for egg",
  "eggPlugin": {
    "name": "tracer"
  },
  "main": "lib/tracer.js",
  "files": [
    "app",
    "config",
    "lib"
  ],
  "keywords": [
    "egg",
    "plugin",
    "tracer",
    "traceId",
    "spanId",
    "parentSpanId"
